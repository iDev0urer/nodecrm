.form-block
  display: block
  width: 100%
  padding: input-padding-y input-padding-x
  font-size: font-size-base
  line-height: line-height
  color: input-color
  background-color: input-bg
  background-image: none
  border: input-btn-border-width solid input-border-color
  border-radius(input-border-radius)
  box-shadow(input-box-shadow)

  &.block-inline
    display: inline-block


  &::-ms-expand {
    background-color: transparent
    border: 0
  }

  &::placeholder
    color: input-color-placeholder
    opacity: 1

  &:disabled,
  &[readonly]
    background-color: input-bg-disabled
    opacity: 1

  &:disabled
    cursor: cursor-disabled

.form-block-file,
.form-block-range
  display: block

.form-block-label
  padding: input-padding-y input-padding-x
  margin-bottom: 0


@media screen and (-webkit-min-device-pixel-ratio: 0)
  input[type="date"],
  input[type="time"],
  input[type="datetime-local"],
  input[type="month"]
    &.form-control
      line-height: $input-height

    &.input-sm,
    .input-group-sm &.form-control
      line-height: $input-height-sm

    &.input-lg,
    .input-group-lg &.form-control
      line-height: $input-height-lg

.form-block-sm
  padding: input-padding-y-sm input-padding-x-sm
  font-size: font-size-sm
  line-height: line-height-sm
  border-radius: border-radius(input-border-radius-sm)

.form-block-lg
  padding: input-padding-y-lg input-padding-x-lg
  font-size: font-size-lg
  line-height: line-height-lg
  border-radius: border-radius(input-border-radius-lg)

.form-group
  margin-bottom: form-group-margin-bottom

// Check Boxes and Radios

.checkbox-styled + label,
.radio-styled + label
  vertical-align: middle
  vertical-align: -webkit-baseline-middle
  display: inline-block
  height: 16px
  width: 16px
  padding: 0
  background-size: 100%
  background-repeat: no-repeat

.checkbox-styled,
.radio-styled
    display:none

.checkbox-styled + label
    background-image: url('data:image/png;base64,')

.checkbox-styled:checked + label
    background-image: url('data:image/png;base64,')
    width: 17px


.radio-styled:checked:disabled + label
  background-image: url('data:image/png;base64,')
  width: 17px

.radio-styled:disabled + label
  background-image: url('data:image/png;base64,')

.radio-styled + label
    background-image: url('data:image/png;base64,')

.radio-styled:checked + label
    background-image: url('data:image/png;base64,')

.radio-styled:checked:disabled + label
  background-image: url('data:image/png;base64,')

.radio-styled:disabled + label
  background-image: url('data:image/png;base64,')
